{
  "manifest_version": 2,
  "name": "FarsiCP",
  "version": "1.2.0",
  "description": "translate competitive programming problems",
  "permissions":[
    "*://*.github.io/*"
  ],
  "content_scripts": [
    {
      "matches": ["*://codeforces.com/*"],
      "js": ["codeforces.js"]
    },
    {
      "matches": ["*://projecteuler.net/*"],
      "js": ["projecteuler.js"]
    },
    {
      "matches": ["*://cp-algorithms.com/*"],
      "js": ["cpalgo.js"]	
    }
  ]

}
